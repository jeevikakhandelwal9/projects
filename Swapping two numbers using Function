class Demo
{
	public static void main(String args[])
	{
		swapFunction(4,1);
	}
	public static void swapFunction(int a,int b) {
		System.out.println("Enter the values of A and B");
		System.out.println("Before Swapping"+a+" "+b );
		int c=a;
		a=b;
		b=c;
		System.out.println("After Swapping"+a+" "+b );
	}
}
