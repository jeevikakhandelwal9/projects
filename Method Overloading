class Demo
{
	public static void main(String args[])
	{
		Demo d=new Demo();
		
		int length=d.area(4,1);
		float breadth=d.area(6);
		System.out.println("Length of the Area is::"+length);
		System.out.println("Breadth of the Area is::"+breadth);
	}
	public static int area(int a,int b) {
			int length=a*b;
			return length;
	}
	public static float area(float n) {
			float breadth=3.142f*n*n;
			return breadth;
	}
}
