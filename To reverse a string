import java.util.Scanner;
public class Demo{
	public static void main(String[] args) {
		Scanner scan=new Scanner(System.in);
		String s=scan.nextLine();
		char[] y=s.toCharArray();
		int size =y.length;
		char[] a=new char[size];
		int i=0;
		while(i!=size) {
			a[size-1-i]=y[i];
			i++;
		}
		System.out.println(y);
		System.out.println(a);
	}
}
